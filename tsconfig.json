{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "Components/*": [
        "src/components/*"
      ],
      "Actions/*": [
        "src/actions/*"
      ],
      "Modules/*": [
        "src/modules/*"
      ],
      "Reducers/*": [
        "src/reducers/*"
      ],
      "App/*": [
        "src/app/*"
      ],
      "Reducers": [
        "src/reducers"
      ],
      "Store": [
        "src/store"
      ],
      "API": [
        "src/api"
      ],
      "API/*": [
        "src/api/*"
      ],
      "Utils": [
        "src/utils"
      ]
    }
  }
}
