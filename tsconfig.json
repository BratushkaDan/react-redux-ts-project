{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "Components/*": [
        "src/app/components/*"
      ],
      "Containers/*": [
        "src/app/containers/*"
      ],
      "Actions/*": [
        "src/app/actions/*"
      ],
      "Layouts/*": [
        "src/app/layouts/*"
      ],
      "Pages/*": [
        "src/app/pages/*"
      ],
      "Modules/*": [
        "src/app/modules/*"
      ],
      "Reducers/*": [
        "src/app/reducers/*"
      ]
    }
  }
}
